{

	"manifest_version": 2,
	"name": "Depression Detection",
	"description": "This extension is predicts a depression rating based on browser history",
	"version": "1.0",
	"icons":{
		"128":"icon128.png",
		"48":"icon48.png",
		"16":"icon16.png"
	},

	"browser_action": 
	{
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},


	"permissions": [
		"activeTab",
		"https://ajax.googleapis.com/"
	]
}